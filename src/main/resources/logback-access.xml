<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="console" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%h %l %u %t "%r" %s %b "%i{Referer}"</pattern>
        </encoder>
    </appender>
    <appender-ref ref="console"/>
</configuration>
